	<content><![CDATA[
vector<int> BIT(N, 0);
void add(int i, int x) {
	for (; i < N; i += i & (-i)) BIT[i]+=x;
}
int get(int i) {
	int sum = 0;
	for (; i > 0; i -= i & (-i)) sum += BIT[i];
	return sum;
}
]]></content>
